<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.HBox?>

<HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.moj.obieg.controllers.MenuScreenController">
   <children>
      <ToggleButton mnemonicParsing="false" onAction="#newUser" text="%title.newUser">
         <toggleGroup>
            <ToggleGroup fx:id="toggleButtons" />
         </toggleGroup></ToggleButton>
      <ToggleButton mnemonicParsing="false" onAction="#newDevice" text="%title.newDevice" toggleGroup="$toggleButtons" />
      <ToggleButton mnemonicParsing="false" onAction="#seeUser" text="%title.seeUser" toggleGroup="$toggleButtons" />
      <ToggleButton mnemonicParsing="false" onAction="#seeDevice" text="%title.seeDevice" toggleGroup="$toggleButtons" />
      <ToggleButton mnemonicParsing="false" onAction="#checkUser" text="%title.checkUser" toggleGroup="$toggleButtons" />
      <ToggleButton mnemonicParsing="false" onAction="#checkDevice" text="%title.seeDevice" toggleGroup="$toggleButtons" />
      <ToggleButton mnemonicParsing="false" onAction="#hardwareRelease" text="%title.hardwareC" toggleGroup="$toggleButtons" />
      <ToggleButton mnemonicParsing="false" onAction="#blockUser" text="%title.blockUser" toggleGroup="$toggleButtons" />
      <ToggleButton mnemonicParsing="false" onAction="#blockDevice" text="%title.blockDevice" toggleGroup="$toggleButtons" />
      <ToggleButton mnemonicParsing="false" onAction="#closeApp" text="%title.close" toggleGroup="$toggleButtons" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</HBox>
